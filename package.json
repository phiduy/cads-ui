      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "cads-ui",
  "author": "Vo Phi Nhat Duy",
  "module": "dist/cads-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/cads-ui.cjs.production.min.js",
